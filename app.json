{
  "expo": {
    "name": "FinancialApp",
    "slug": "financial-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.meena1220.financialapp",
      "config": {
        "googleMapsApiKey": "YOUR_IOS_GOOGLE_MAPS_API_KEY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.meena1220.financialapp",
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_ANDROID_GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "output": "single"
    },
    "owner": "minaabararghi",
    "extra": {
      "eas": {
        "projectId": "24fda234-ea74-4017-b9ce-0be0b6d173b3"
      }
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal"
      }
    }
  }
}
